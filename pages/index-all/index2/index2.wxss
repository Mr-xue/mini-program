/* components/index2/index2.wxss */
.s3 {
	margin-bottom: 100rpx;
	padding-bottom: 100rpx;
}

.f-ele {
	position: fixed;
	right: 10px;
	bottom: 100px;
	font-size: 11px;
	text-align: center;
	width: 50px;
	height: 50px;
	line-height: 50px;
	border-radius: 50%;
	background-color: #434349;
	color: #fff;
}

.long {
	height: 800rpx;
}
.works {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	padding: 10rpx 30rpx;
}
.w-li {
	width: 330rpx;
	margin-bottom: 20rpx;
}
.w-li > image {
	display: block;
	width: 330rpx;
	height: 330rpx;
}
.w-tit {
	padding-top: 5px;
	font-size: 28rpx;
	color: #333;
}
.w-time {
	font-size: 24rpx;
	color: #aaa;
}
.work-tit {
	position: relative;
	font-weight: bold;
	color: #333;
	padding-left: 60rpx;
	margin-top: 40rpx;
}
.work-tit::before {
	content: "";
	position: absolute;
	left: 30rpx;
	top: 12rpx;
	width: 8rpx;
	height: 28rpx;
	background: #82c8af;
}
